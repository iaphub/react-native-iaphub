{
  "extends": "expo/tsconfig.base.json",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ],
      "react-native-iaphub": [
        "../../src"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "../../src/**/*.ts",
    "../../src/**/*.tsx"
  ]
}
